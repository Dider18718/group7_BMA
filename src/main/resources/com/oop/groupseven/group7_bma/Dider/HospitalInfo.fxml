<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.oop.groupseven.group7_bma.Dider.HospitalInfoController"
            prefHeight="400.0" prefWidth="600.0">
    <children>
        <Label layoutX="20" layoutY="20" text="Hospital Info Settings"/>
        <TextField fx:id="contactField" layoutX="20" layoutY="60" promptText="Contact Number"/>
        <TextField fx:id="addressField" layoutX="20" layoutY="100" promptText="Address"/>
        <TextField fx:id="servicesField" layoutX="20" layoutY="140" promptText="New Service (optional)"/>
        <Button text="Save" layoutX="20" layoutY="180" onAction="#save"/>
        <Label fx:id="statusLabel" layoutX="20" layoutY="210"/>
    </children>

</AnchorPane>
