<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.oop.groupseven.group7_bma.Dider.RegisterPatientController"
            prefHeight="400.0" prefWidth="600.0">
    <children>
        <Label layoutX="20" layoutY="20" text="Register Patient"/>
        <TextField fx:id="nameField" layoutX="20" layoutY="60" promptText="Name"/>
        <TextField fx:id="ageField" layoutX="20" layoutY="100" promptText="Age"/>
        <TextField fx:id="contactField" layoutX="20" layoutY="140" promptText="Contact Number"/>
        <Button text="Register" layoutX="20" layoutY="180" onAction="#registerPatient"/>
        <Label fx:id="statusLabel" layoutX="20" layoutY="220"/>
    </children>

</AnchorPane>
